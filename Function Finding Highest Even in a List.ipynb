    "def highest_even(list):\n",
    "    evens = []\n",
    "    for number in list:\n",
    "        if number %2 == 0:\n",
    "            evens.append(number)\n",
    "            \n",
    "    return max(evens)\n",
    "    \n",
    "print(highest_even([10,2,3,4,8,11]))"
   ]
